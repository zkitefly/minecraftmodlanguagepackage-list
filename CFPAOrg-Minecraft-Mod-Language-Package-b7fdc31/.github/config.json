    "runners": [
        {
            "Root": "*",
            "projectType": "other",
            "versioning": {
                "source": "milestones",
                "type": "SemVer"
            },
            "prereleaseName": "release",
            "sharedConfig": {
                "labels": {
                    "bug": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "titleMatches",
                                "pattern": "/^bug(\\(.*\\))?(!)?:/i"
                            },
                            {
                                "type": "descriptionMatches",
                                "pattern": "/(created|new|opened|made)( an| a)? bug/i"
                            }
                        ]
                    }
                }
            },
            "pr": {
                "labels": {
                    "xs": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "changesSize",
                                "min": 0,
                                "max": 10
                            }
                        ]
                    },
                    "s": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "changesSize",
                                "min": 10,
                                "max": 40
                            }
                        ]
                    },
                    "m": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "changesSize",
                                "min": 40,
                                "max": 100
                            }
                        ]
                    },
                    "l": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "changesSize",
                                "min": 100,
                                "max": 500
                            }
                        ]
                    },
                    "xl": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "changesSize",
                                "min": 500,
                                "max": 1000
                            }
                        ]
                    },
                    "xxl": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "changesSize",
                                "min": 1000,
                                "max": 2000
                            }
                        ]
                    },
                    "xxxl": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "changesSize",
                                "min": 2000,
                                "max": 5000
                            }
                        ]
                    },
                    "xxxxl": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "changesSize",
                                "min": 5000
                            }
                        ]
                    },
                    "wip": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "isDraft",
                                "value": true
                            },
                            {
                                "type": "titleMatches",
                                "pattern": "(?<=\\[)WIP(?=\\])"
                            }
                        ]
                    },
                    "hard": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "titleMatches",
                                "pattern": "(?<=\\[)P(?=\\])"
                            }
                        ]
                    },
                    "config": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "filesMatch",
                                "glob": "config/**"
                            }
                        ]
                    },
                    "source": {
                        "requires": 1,
                        "conditions": [
                            {
                                "type": "filesMatch",
                                "glob": "src/**"
                            }
                        ]
                    }
                }
            }
        }
    ]
}
